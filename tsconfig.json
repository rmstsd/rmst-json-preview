{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "importHelpers": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": "./",
        "strict": true,
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@@/*": [
                "src/.umi/*"
            ]
        },
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "lib",
        "webpackConfig",
        "demo"
    ]
}